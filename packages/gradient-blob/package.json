{
  "name": "@yuvalkarif/gradient-blob",
  "type": "module",
  "version": "0.0.0",
  "description": "simple generators for creating random css blobs",
  "author": "Yuval Karif",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuvalkarif/gradient-blob.git",
    "directory": "packages/gradient-blob"
  },
  "keywords": ["CSS", "blob", "gradient", "clip-path", "ts", "js"],
  "imports": {
    "#root/*": "./dist/*"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.js",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "dev": "rm -rf ./dist && tsc --watch --preserveWatchOutput",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "typescript": "^5.4.5"
  }
}
